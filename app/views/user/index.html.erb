<<div class="col-md-3">
<%= render 'layouts/userinfo', user: @user %>
<%= render 'layouts/newbook', book: @book %>
</div>
<div class="col-md-8 offset-md-1">
    <h2>Users </h2>
<% @users.each do |user| %>
<h5>image</h5>
<td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></td>
<h5>name</h5>
      <td><%= user.name %></td>
<%= link_to "Show", user_path(user.id) %>
<% end %>
</div>